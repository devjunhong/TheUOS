    <!-- Widget & etc -->
    <eat-comment/>
    <style name="AppWidget.Inverse.Light" parent="@style/Theme.AppCompat.Light">
        <item name="android:textColorPrimary">?color_actionbar_title</item>
        <item name="colorControlNormal">?color_actionbar_title</item>
        <item name="android:textColorHint">?color_actionbar_title</item>
        <item name="android:textColorPrimaryInverse">?color_actionbar_title</item>
        <item name="colorControlActivated">?color_actionbar_title</item>
        <item name="colorControlHighlight">?color_actionbar_title</item>

    </style>

