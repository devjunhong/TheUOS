        <item name="android:textColorPrimary">?color_actionbar_title</item>
        <item name="colorControlNormal">?color_actionbar_title</item>
    </style>

    <style name="AppWidget.SearchView" parent="Widget.AppCompat.SearchView">
        <item name="android:maxHeight">?actionBarSize</item>
        <item name="android:textColorPrimary">?color_actionbar_title</item>
        <item name="colorControlNormal">?color_actionbar_title</item>
    </style>

    <style name="menu_labels_style">
        <item name="android:background">@drawable/fab_label_background</item>
        <item name="android:textColor">@android:color/white</item>
    </style>


    <!-- ActionBar -->
    <eat-comment/>
    <style name="AppWidget.ActionBar.ActionBarStyle.Light" parent="@style/Widget.AppCompat.Light.ActionBar">
        <item name="titleTextStyle">@style/AppWidget.ActionBar.TitleTextStyle</item>
        <item name="subtitleTextStyle">@style/AppWidget.ActionBar.SubTitleTextStyle</item>
        <item name="android:textColorPrimary">?color_actionbar_title</item>
        <item name="colorControlNormal">?color_actionbar_title</item>

    </style>

