    <style name="AppTheme.Light.DarkActionBar" parent="@style/AppTheme.Base.Light.DarkActionBar">
    </style>


    <style name="AppTheme.Light" parent="@style/AppTheme.Base.Light">
    </style>

    <style name="AppTheme.Dark" parent="@style/AppTheme.Base.Dark">
