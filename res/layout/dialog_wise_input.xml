<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <EditText
        android:id="@+id/dialog_wise_id_input"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/dialog_wise_id"
        android:layout_marginLeft="21dp"
        android:ems="10"
        android:inputType="text">
        <requestFocus />
    </EditText>

    <TextView
        android:id="@+id/dialog_wise_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/dialog_wise_id_input"
        android:layout_alignParentTop="true"
        android:text="@string/tab_timetable_wise_id" />

    <TextView
        android:id="@+id/dialog_wise_passwd_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/dialog_wise_id_input"
        android:layout_below="@+id/dialog_wise_id_input"
        android:text="@string/tab_timetable_wise_passwd" />

    <EditText
        android:id="@+id/dialog_wise_passwd_input"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/dialog_wise_passwd_text"
        android:layout_below="@+id/dialog_wise_passwd_text"
        android:ems="10"
        android:inputType="textPassword" >

        
    </EditText>

</RelativeLayout>